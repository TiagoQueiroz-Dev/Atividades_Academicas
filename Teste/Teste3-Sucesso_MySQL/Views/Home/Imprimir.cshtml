@model List<Cadastro>


<h1>Listar</h1>
@foreach(var item in Model){
    <table class = "table table-borderless">
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Nome</th>
            <th scope="col">Sobrenome</th>
        </tr>
        <tr>
            <th scope="row">@item.Id</th>
            <td>@item.Nome</td>
            <td>@item.Sobrenome</td>
        </tr>
    </table>
}